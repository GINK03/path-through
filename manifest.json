{
  "name": "Path-Through",
  "version": "0.0.2",
  "manifest_version": 2,
  "description": "this is a data strage linker for each persons",
  "icons":{
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "browser_action":{
    "default_icon":{
      "19":"icons/icon16.png"
    },
    "default_title":"Path-Through",
    "default_popup":"popup.html"
  },
  "background": {
      "scripts": ["background.js"]
  },
  "content_scripts":[
    {
      "matches":["<all_urls>"],
      "js":["jquery-2.1.4.min.js","content-script.js","embeded-script.js"],
      "all_frames": true,
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources":["jquery-2.1.4.min.js", "content-script.js","embeded-script.js"],
  "permissions":[
    "alarms",
    "<all_urls>",
    "unlimitedStorage",
    "storage"
  ]
}
